#include <stdio.h>

int main() {
	printf("samivulla\n192321078\n");
    int arr[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr) / sizeof(arr[0]);
    int element = 30;
    int location = -1;

    for (int i = 0; i < n; i++) {
        if (arr[i] == element) {
            location = i;
            break;
        }
    }

    printf("The element %d is located at index %d in the array.", element, location);

    return 0;
}
